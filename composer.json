{
    "name": "sql-analyzer/sql-analyzer",
    "description": "A Laravel package for an SQL editor and visualizer with autocomplete and paginated results.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SqlAnalyzer\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "SqlAnalyzer\\SqlAnalyzerServiceProvider"
            ]
        }
    },
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
